<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sist.data.mapper.PartnerServiceMapperInter">

   <insert id="insertPartner" parameterType="partner">
     insert into partner (partner_name,partner_gender,partner_birth,partner_phone,partner_addr,partner_smoking,
     partner_job,partner_date,partner_time,partner_exp_cat,partner_exp_dog,partner_exp)
     values(#{partner_name},#{partner_gender},#{partner_birth},#{partner_phone},#{partner_addr},#{partner_smoking},
     #{partner_job},#{partner_date},#{partner_time},#{partner_exp_cat},#{partner_exp_dog},#{partner_exp})
   </insert>
  
   <select id="PartnerList" resultType="partner">
     select * from partner
   </select>
</mapper>